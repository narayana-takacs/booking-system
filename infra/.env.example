# n8n & Postgres stack
N8N_ENCRYPTION_KEY=replace-with-32-char-secret
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change-me
N8N_HOST=chatgpt-n8n.local
WEBHOOK_URL=https://chatgpt-n8n.local/
GENERIC_TIMEZONE=Etc/UTC
DB_TYPE=postgresdb
POSTGRES_PASSWORD=change-me-too
POSTGRES_USER=n8n
POSTGRES_DB=n8n

# Airtable configuration
AIRTABLE_PAT=patXXXX
AIRTABLE_BASE_ID=appXXXXXXXXXXXXXX
AIRTABLE_TABLE_AVAILABILITY=Availability
AIRTABLE_TABLE_BOOKINGS=Bookings
AIRTABLE_TABLE_CLIENTS=Clients

# SMTP (Mailpit default for development)
SMTP_HOST=mailpit
SMTP_PORT=1025
SMTP_USER=
SMTP_PASSWORD=
PROVIDER_ALERT_EMAIL=provider@example.com
AIRTABLE_USE_STUB=false
